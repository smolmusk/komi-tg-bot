{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "../public"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "http://172.16.13.2:3001/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "VITE_API_URL": "http://172.16.13.2:3001",
    "VITE_NODE_ENV": "production"
  }
}
